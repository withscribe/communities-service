# Author: Austin Howlett
# Description: Database models

type Community {
  id: ID! @unique
  ownerId: ID
  moderatorId: String @default(value: "No Moderator")
  name: String! @unique
  description: String
  createdAt: DateTime!
  updatedAt: DateTime!
  membersIds: [ID!]!
  bannedMembersIds: [ID!]!
  privacy: Privacy @default(value: PUBLIC)
  storyIds: [ID!]!
}

enum Privacy {
  PUBLIC
  PRIVATE
  INVITE_ONLY
}
